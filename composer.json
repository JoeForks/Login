{
    "name": "styleci/login",
    "description": "StyleCI Login Is A GitHub Login Provider",
    "keywords": ["login", "github", "StyleCI", "StyleCI", "Login", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/http": "~5.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "suggest": {
        "illuminate/support": "Enables Laravel support (~5.0)."
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\Login\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
